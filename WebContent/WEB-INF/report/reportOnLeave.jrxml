	<queryString>
		<![CDATA[SELECT US.[ID],US.[TITLE_NAME],US.[FIRST_NAME],US.[LAST_NAME],US.[DEPARTMENT],US.[POSITION],US.[EMAIL],BOSS.*,LV.[CODE]
,LV.[EMPLOYEE_ID],LV.[LEAVE_TYPE],LV.[START_DATE],LV.[END_DATE],LV.[NUM_LEAVE],LV.[NOTE]
,ISNULL((SELECT SUM (CASE WHEN LV.LEAVE_TYPE = 'ลาป่วย' THEN NUM_LEAVE ELSE 0 END) FROM LEAVE_MST_LEAVE LV WHERE US.[ID]=LV.[EMPLOYEE_ID] AND LV.[CODE] != $P{code} AND LV.[EMPLOYEE_ID] = $P{id}),0) AS BEF_SICK_LEAVE
,ISNULL((SELECT SUM (CASE WHEN LV.LEAVE_TYPE = 'ลากิจ' THEN NUM_LEAVE ELSE 0 END) FROM LEAVE_MST_LEAVE LV WHERE US.[ID]=LV.[EMPLOYEE_ID] AND LV.[CODE] != $P{code} AND LV.[EMPLOYEE_ID] = $P{id}),0) AS BEF_ERRAND_LEAVE
,ISNULL((SELECT SUM (CASE WHEN LV.LEAVE_TYPE = 'ลาพักร้อน' THEN NUM_LEAVE ELSE 0 END) FROM LEAVE_MST_LEAVE LV WHERE US.[ID]=LV.[EMPLOYEE_ID] AND LV.[CODE] != $P{code} AND LV.[EMPLOYEE_ID] = $P{id}),0) AS BEF_VACATION_LEAVE
,ISNULL((SELECT SUM (CASE WHEN LV.LEAVE_TYPE = 'ลาป่วย' THEN NUM_LEAVE ELSE 0 END) FROM LEAVE_MST_LEAVE LV WHERE US.[ID]=LV.[EMPLOYEE_ID] AND LV.[CODE] = $P{code} AND LV.[EMPLOYEE_ID] = $P{id}),0) AS THIS_SICK_LEAVE
,ISNULL((SELECT SUM (CASE WHEN LV.LEAVE_TYPE = 'ลากิจ' THEN NUM_LEAVE ELSE 0 END) FROM LEAVE_MST_LEAVE LV WHERE US.[ID]=LV.[EMPLOYEE_ID] AND LV.[CODE] = $P{code} AND LV.[EMPLOYEE_ID] = $P{id}),0) AS THIS_ERRAND_LEAVE
,ISNULL((SELECT SUM(CASE WHEN LV.LEAVE_TYPE = 'ลาพักร้อน' THEN NUM_LEAVE ELSE 0 END) FROM LEAVE_MST_LEAVE LV WHERE US.[ID]=LV.[EMPLOYEE_ID] AND LV.[CODE] = $P{code} AND LV.[EMPLOYEE_ID] = $P{id}),0) AS THIS_VACATION_LEAVE
FROM LEAVE_MST_USER US JOIN LEAVE_MST_LEAVE LV ON US.[ID]=LV.[EMPLOYEE_ID] JOIN (SELECT US.[ID] AS ID,US.[TITLE_NAME] AS BTITLE_NAME,
US.[FIRST_NAME] AS BFIRST_NAME,US.[LAST_NAME] AS BLAST_NAME,US.[DEPARTMENT] AS B_DEPARTMENT,US.[POSITION] AS B_POSITION FROM LEAVE_MST_USER US WHERE US.[POSITION]='หัวหน้าพนักงาน') BOSS ON BOSS.[ID]=LV.[BOSS_ID] WHERE LV.[CODE] = $P{code} AND LV.[EMPLOYEE_ID] = $P{id};]]>
	</queryString>
